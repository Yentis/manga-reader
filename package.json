{
  "name": "quasar-manga-reader",
  "version": "2.24.2",
  "description": "Manga reader that scrapes manga sites for updates",
  "productName": "Manga Reader",
  "author": "Yentis#5218",
  "private": true,
  "scripts": {
    "lint": "eslint ./",
    "build:android": "node --max-old-space-size=8192 node_modules/@quasar/cli/bin/quasar build -m capacitor -T android",
    "build:ios": "node --max-old-space-size=8192 node_modules/@quasar/cli/bin/quasar build -m capacitor -T ios",
    "sign:android": "apksigner sign --verbose --ks my-release-key.keystore --ks-key-alias manga_reader ./dist/capacitor/android/apk/release/app-release-unsigned.apk",
    "build:electron": "node --max-old-space-size=8192 node_modules/@quasar/cli/bin/quasar build -m electron",
    "build:ssg": "quasar ssg generate",
    "serve:ssg": "quasar ssg serve dist/ssg"
  },
  "@comments": {
    "devDependencies": {
      "@babel/core": "Peer dependency for @babel/eslint-parser",
      "@types/node-fetch": "Peer dependency for dropbox",
      "buffer": "Peer dependency for quasar-app-extension-ssg > crc",
      "jsdom": "Peer dependency for jsdom-global",
      "typescript": "Peer dependency for @typescript-eslint/eslint-plugin > tsutils",
      "webpack": "Peer dependency for node-polyfill-webpack-plugin"
    }
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.26.6",
    "@quasar/cli": "^1.2.1",
    "@quasar/extras": "^1.0.0",
    "@zip.js/zip.js": "^2.7.24",
    "core-js": "^3.6.5",
    "dropbox": "^10.28.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.29.2",
    "node-polyfill-webpack-plugin": "^1.1.3",
    "p-queue": "^6.6.2",
    "quasar": "2",
    "relevancy": "^0.2.0",
    "vue": "3",
    "vue-router": "4",
    "vuex": "4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@capacitor/core": "^6.0.0",
    "@capacitor/local-notifications": "^6.1.1",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
